import turtle
turtle.setup(800,800,0,0)
turtle.pensize(5)
turtle.speed(3)
turtle.fillcolor("blue")
turtle.left(90)
turtle.forward(100)
turtle.circle(10,500)
turtle.penup()
turtle.goto(0,0)
turtle.pendown()
turtle.goto(0,0)
turtle.goto(0,-200)
turtle.goto(200,-200)
turtle.write("我是夜神月")



